<head>
    <title>The Hair Salon</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    <link rel="stylesheet" type="text/css" href="~/css/styles.css" />
</head>
<body>
  <div class="container">
    <h1>List of Stylists</h1>
    @foreach(var person in Model)
    {
      <li>Name: @person.GetName(), Style: @person.GetStyle()</li>
    }
    <form action="/Stylist/CreateForm">
      <button class="btn btn-success" type="click">Add New Stylist</button>
    </form>
    <form action="/Client">
      <button class="btn btn-success" type="click">View Client List</button>
    </form>
  </div>
</body>
